@model IEnumerable<AIS.Models.get_Posts_Result>
@foreach (var item in Model)
{
    <h3>@Html.Raw(HttpUtility.HtmlDecode(item.Title))</h3>
    
        @Html.Raw(HttpUtility.HtmlDecode(item.ShortDescription))
    <p>
        @Html.Raw(HttpUtility.HtmlDecode(item.Meta))<br />
        @Html.Raw(HttpUtility.HtmlDecode(item.UrlSlug))<br />
        @Html.Raw(HttpUtility.HtmlDecode(item.LastModified.ToString()))<br />
        @Html.ActionLink("Details", "Details", new { id = item.Id })<br />
    </p>
}
